<div class="row">
  <div class="col-md-6 .col-md-offset-3">
    
  <% @users.each do |user| %>
    <%if user == current_user %>
      <%next%>
    <% elsif current_user.friended_users.ids.include?(user.id)%>
      <%next%> 
    <%else%>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><%=user.username%></h3>
      </div>
      <div class="panel-body">
      <button type="button" class="btn btn-primary"><%=link_to "Friend User", new_friending_path(:id => user.id)%></button>
      </div>
    </div>
    <%end%>
  <%end%>

  </div>
</div>